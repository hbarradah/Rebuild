<!DOCTYPE html>
    <Head>
      <Title> <%= title %> </Title>
      <%= csrf_meta_tag %>
      <%= render 'layouts/stylesheets' %>
    </Head>

    <body>
        <div class="container">
            <%= render 'layouts/header'%>
            <section class="round">
                <%= yield%>
            </section>
            <%= render 'layouts/footer' %>
            <%= debug(params) if Rails.env.development? %>
        </div>
    </body>
</html>